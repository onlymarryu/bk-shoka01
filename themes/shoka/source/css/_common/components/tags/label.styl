span.label {
  display: inline;
  border-radius: .3rem;
  border: .0625rem solid;
  padding: .2rem .3rem;
  font-family: $font-family-monospace;
  font-size: $font-size-medium;

  background: var(--note-bg, var(--grey-2));
  border-color: var(--note-border, var(--grey-4));
  color: var(--note-text, var(--grey-6));
}
